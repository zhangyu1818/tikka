{
  "private": true,
  "version": "0.0.0-alpha.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:packages": "cjsb -d --packages packages/shared packages/transform packages/transform-less packages/transform-sass packages/with-postcss packages/declaration packages/compile packages/tikka",
    "lint:script": "eslint packages/**/src/*.ts",
    "lint:fix": "yarn run lint:script --fix",
    "tsc:check": "tsc --noEmit",
    "prettier": "prettier --write packages/**/*",
    "test": "jest",
    "release": "node ./scripts/release.js",
    "publish:packages": "yarn workspaces foreach --no-private npm publish",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@swc/core": "^1.2.105",
    "@swc/jest": "^0.2.5",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "autoprefixer": "^10.3.1",
    "cjsb": "0.0.8",
    "enquirer": "^2.3.6",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.1.1",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "semver": "^7.3.5",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
